te(false);\n\n  useEffect(() => {\n    loadGoals();\n  }, []);\n\n  function loadGoals() {\n    API.getGoals()\n      .then((res) => setGoals(res.data))\n      .catch((err) => console.log(err));\n  }\n\n  function deleteGoal(id) {\n    setConfettiRun(true);\n    API.deleteGoal(id)\n      .then((res) => loadGoals())\n      .catch((err) => console.log(err));\n  }\n\n  const { width, height } = useWindowSize();\n\n  return (\n    <div>\n      <Confetti\n        width={width}\n        height={height}\n        run={confettiRun}\n        numberOfPieces={2000}\n        recycle={false}\n      />\n      <h1 style={{ textAlign: \"center\" }}>\n        <a href=\"/goals\">My Goals</a>\n      </h1>\n      {goals.length ? (\n        <List>\n          {goals.map((goal) => (\n            <ListItem key={goal._id}>\n              {goal.goal}\n              <DeleteBtn\n                deleteText=\"I did it!\"\n                onClick={() => deleteGoal(goal._id)}\n              />\n            </ListItem>\n          ))}\n        </List>\n      ) : (\n        <h3>\n          {\" \"}\n          <br />\n          No goals yet... Click{\" \"}\n          <a href=\"/goals\">\n            <u>here</u>\n          </a>{\" \"}\n          to get started!\n        </h3>\n      )}\n    </div>\n  );\n}\n\nexport default GoalsList;\n","/Users/graelphick/Google Drive/CODING BOOTCAMP/Projects/Project 3/motivator/client/src/components/ResourcesList/index.js",["157","158"],"/Users/graelphick/Google Drive/CODING BOOTCAMP/Projects/Project 3/motivator/client/src/components/Col/index.js",[],"/Users/graelphick/Google Drive/CODING BOOTCAMP/Projects/Project 3/motivator/client/src/components/Row/index.js",[],"/Users/graelphick/Google Drive/CODING BOOTCAMP/Projects/Project 3/motivator/client/src/components/Container/index.js",[],"/Users/graelphick/Google Drive/CODING BOOTCAMP/Projects/Project 3/motivator/client/src/pages/NewGoal.js",[],["159","160"],"/Users/graelphick/Google Drive/CODING BOOTCAMP/Projects/Project 